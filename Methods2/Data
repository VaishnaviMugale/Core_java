package Methods2;

public class Data {

	int x,y,z; //instance variable
	
	public void setX (int x) {
	this.x=x;
}
	public void setY (int y) {
		this.y=y;
	}
	public void setZ (int z) {
		this.z=z;
	}
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	public int getZ() {
		return z;
	}
}
